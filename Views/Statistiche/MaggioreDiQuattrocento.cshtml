@{
}

<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Data</th>
            <th scope="col">Indirizzo</th>
            <th scope="col">Data Trascrizione</th>
            <th scope="col">Importo</th>
            <th scope="col">Decurtamento Punti</th>
            <th scope="col">Decurtamento Punti</th>
            <th scope="col">Id Anagrafica</th>
            <th scope="col">Id Tipo Violazione</th>
        </tr>
    </thead>

    <tbody>

        @foreach (var row in Model)
        {
            <tr>
                <td scope="row">@row["IdVerbale"]</td>
                <td>@row["DataViolazione"]</td>
                <td>@row["IndirizzoViolazione"]</td>
                <td>@row["Nominativo_Agente"]</td>
                <td>@row["DataTrascrizioneVerbale"]</td>
                <td>@row["Importo"]</td>
                <td>@row["DecurtamentoPunti"]</td>
                <td>@row["AnagraficaId"]</td>
                <td>@row["TipoViolazioneId"]</td>
            </tr>

        }
    </tbody>
</table>